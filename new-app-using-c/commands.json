{
    "commands": [
        {
            "name": "pwd",
            "description": "Display the current working directory",
            "category": "navigation",
            "bookmarked": false,
            "flags": {},
            "examples": {
                "basic": {
                    "command": "pwd",
                    "output": "/home/user/Desktop"
                }
            }
        },
        {
            "name": "ls",
            "description": "Lists files and directories in current directory or specified directory if provided",
            "category": "navigation",
            "bookmarked": false,
            "flags": {
                "-l": "Long format ",
                "-a": "Display all files",
                "-h": "Display readable file sizes",
                "-t": "Sort by modification time",
                "-r": "Display items in reverse order",
                "-S": "Sort by file size",
                "-R": "Lists subfiles and subdirectories"
            },
            "examples": {
                "basic": {
                    "command": "ls",
                    "output": "directory1 directory2 directory3 file1.md file2.py file3.js"
                },
                "with_flags": {
                    "ls -l": {
                        "purpose": "Display the directories and files in long format. Display the permissions, number of hard links, owner, group, file size, last modification date and file name",
                        "output": "-rw-r--r-- 1 user user 71740 Dec 24 20:04 file1.md"
                    },
                    "ls -a": {
                        "purpose": "Display all files, inlcuding hidden files starting with .",
                        "output": "directory1 directory2 directory3 file1.md file2.py file3.js .hiddenDirectory .hiddenFile.txt"
                    },
                    "ls -lh": {
                        "purpose": "Display the directories and files using the -l flag, but display readable file sizes (KB, MB, GB)",
                        "output": "-rw-r--r-- user user 7.1K Dec 24 20:04 file1.md"
                    },
                    "ls -t": {
                        "purpose": "Display the directories and files, organised beginning with the most recently modified item",
                        "output": "file2.py directory2 file3.js directory1 directory3 file1.md"
                    },
                    "ls -r": {
                        "purpose": "Display items in reverse order to default",
                        "output": "directory3 directory2 directory1 file3.js file2.py file1.md"
                    },
                    "ls -S": {
                        "purpose": "Sort items by file size, beginning with the largest file",
                        "output": "directory2 directory1 directory3 file3.js file1.md file2.py"
                    },
                    "ls -R": {
                        "purpose": "List all directories and files in the current directory and each subdirectory",
                        "output": ""
                    }
                }
            }
        },
