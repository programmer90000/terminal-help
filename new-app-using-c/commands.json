                },
                "with_flags": {
                    "mv -i file1 file2": {
                        "purpose": "Interactive move",
                        "output": "mv: overwrite 'file2'? y"
                    },
                    "mv -v *.txt archive/": {
                        "purpose": "Verbose move",
                        "output": "'file1.txt' -> 'archive/file1.txt'\n'file2.txt' -> 'archive/file2.txt'"
                    },
                    "mv -u newfile oldfile": {
                        "purpose": "Update if newer",
                        "output": "File moved only if newer"
                    }
                }
            }
        },
