                },
                "with_flags": {
                    "p": {
                        "command": "mkdir -p projects/2024/src",
                        "output": "",
                        "explanation": "Creates the entire directory path. If 'projects' or '2024' don't exist, they are created automatically."
                    },
                    "m": {
                        "command": "mkdir -m 750 secure_dir",
